# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Supabase Admin (Server-side only - never expose in client!)
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Bunny.net Video Streaming (lessons only; images use Supabase Storage)
# NEXT_PUBLIC_BUNNY_CDN_URL is required for video playback (e.g. https://vz-XXXXX.b-cdn.net)
BUNNY_STREAM_LIBRARY_ID=your_bunny_stream_library_id
BUNNY_STREAM_API_KEY=your_bunny_stream_api_key
NEXT_PUBLIC_BUNNY_CDN_URL=https://vz-XXXXX.b-cdn.net
